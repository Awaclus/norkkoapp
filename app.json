    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ec8be130-6679-433c-b619-cd06377ad290"
      }
    },
    "android": {
      "package": "com.awaclus.norkkoapp"
    }
  }
}
