{
  "expo": {
    "name": "norkkoapp",
    "slug": "norkkoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fdf8e5"
    },
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
